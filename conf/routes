# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#GET     /                           controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

#Ontology
#show all classes name
GET		/onto/all					controllers.MyOntology.all()
#get specific properties
GET    /onto                        controllers.MyOntology.getProperties(classname)

#MyIndividual
GET    /indiv/get					 controllers.MyIndividual.get(classname)
POST   /indiv/add                    controllers.MyIndividual.add()
POST   /indiv/addproperties          controllers.MyIndividual.addProperties()
